       (diff -c s.O $srcdir/s.X) 2> /dev/null
       errors=`expr $errors + 1`;;
    2) $echo "Test s(<) may have failed." 1>&2
       $echo The command "cmp s.O $srcdir/s.X" failed. 1>&2
       errors=`expr $errors + 1`;;
  esac
fi
