       (diff -c ross-4.O $srcdir/ross-4.X) 2> /dev/null
       errors=`expr $errors + 1`;;
    2) $echo "Test ross-4(<) may have failed." 1>&2
       $echo The command "cmp ross-4.O $srcdir/ross-4.X" failed. 1>&2
       errors=`expr $errors + 1`;;
  esac
fi
test -s ross-4.E || rm -f ross-4.E
