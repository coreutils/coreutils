       errors=`expr $errors + 1`;;
    2) $echo "Test obs-c1(_POSIX2_VERSION=199209:F) may have failed." 1>&2
       $echo The command "cmp obs-c1.O $srcdir/obs-c1.X" failed. 1>&2
       errors=`expr $errors + 1`;;
  esac
fi
