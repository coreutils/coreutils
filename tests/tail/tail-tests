    0) if test "$VERBOSE"; then $echo "passed obs-c1(_POSIX2_VERSION=199209:F)"; fi;;
    1) $echo "Test obs-c1(_POSIX2_VERSION=199209:F) failed: files obs-c1.O and $srcdir/obs-c1.X differ" 1>&2
