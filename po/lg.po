"  -a, --all         same as -b -d --login -p -r -t -T -u\n"
"  -b, --boot        time of last system boot\n"
"  -d, --dead        print dead processes\n"
"  -H, --heading     print line of column headings\n"
msgstr ""

#: src/who.c:582
msgid ""
"  -i, --idle        add idle time as HOURS:MINUTES, . or old\n"
"                    (deprecated, use -u)\n"
